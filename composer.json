{
  "name": "m1ge0/bootsjet",
    "description": "A Laravel 10 package for Jetstream + Livewire to switch from Tailwindcss to Bootstrap.",
    "keywords": ["laravel", "bootstrap", "jetstream"],
    "license": "MIT",
    "authors": [
        {
            "name": "m1ge0",
            "role": "Dev"
        }
    ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0"
  },
  "require-dev": {
    "laravel/jetstream": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "M1Ge0\\Bootsjet\\": "src/M1Ge0/Bootsjet/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "M1Ge0\\Bootsjet\\BootsjetServiceProvider"
      ],
      "aliases": {
        "Bootsjet": "M1Ge0\\Bootsjet\\JetstrapFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

